{
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "useTabs": false,
    "overrides": [
      {
        "files": "*.tsx",
        "options": {
          "parser": "typescript"
        }
      },
      {
        "files": "*.jsx",
        "options": {
          "parser": "babel"
        }
      }
    ]
  }
  